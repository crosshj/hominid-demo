{
    "framework": "vite",
    "rewrites": [
        {
            "source": "/api/auth/(.*)",
            "destination": "/api/auth"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api"
        },
        {   
            "source": "/(.*)", 
            "destination": "/" 
        }
    ],
    "functions": {
        "api/*.js": {
          "maxDuration": 10
        }
      }
}
