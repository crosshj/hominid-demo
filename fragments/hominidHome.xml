<Page>
	<Data
		name="pathParams"
		route="/#/:path?"
		defaultValue='{ "path": "home" }'
	/>
	<Data
		name="menus"
		getData="hominidMenus"
	/>
	<Data
		name="fragmentContents"
		getData="ui.sp_UIContextGetComponentsByUserID"
		param_fragment="global_pathParams.path"
	/>
	<Data
		name="mobileMenuOpen"
		defaultValue="false"
	/>

	<Flow key="openMobileMenu">
		<SetData
			name="global_mobileMenuOpen"
			data="true"
		/>
	</Flow>
	<Flow key="closeMobileMenu">
		<SetData
			name="global_mobileMenuOpen"
			data="false"
		/>
	</Flow>

	<Layout
		mobileMenuOpen="global_mobileMenuOpen"
		closeMobileMenuFn="runFlow_closeMobileMenu"
	>
		<Header openMobileMenuFn="runFlow_openMobileMenu">
			<Logo width="150px" />
		</Header>
		<Menu
			items="global_menus"
			selected="global_pathParams.path"
			closeMobileMenuFn="runFlow_closeMobileMenu"
		/>
		<Content>
			<Fragment contents="global_fragmentContents" />
		</Content>
		<Footer>
			<Typography>footer</Typography>
		</Footer>
		<!-- <Sidebar></Sidebar> -->
	</Layout>
</Page>
