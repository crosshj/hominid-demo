<Page>
	<Data
		name="pathParams"
		route="/#/:path?"
		defaultValue='{ "path": "home" }'
	/>
	<Data
		name="menus"
		getData="hominidMenus"
	/>
	<Data
		name="mainContent"
		getData="ui.sp_UIContextGetComponentsByUserID"
		param_fragment="global_pathParams.path"
	/>
	<Data
		name="mobileMenuOpen"
		defaultValue="false"
	/>

	<Flow key="setMobileMenu">
		<SetData
			name="global_mobileMenuOpen"
			data="flowArgs.value"
		/>
	</Flow>

	<Layout
		mobileMenuOpen="global_mobileMenuOpen"
		setMobileMenuFn="runFlow_setMobileMenu"
	>
		<Header setMobileMenuFn="runFlow_setMobileMenu">
			<Logo width="150px" />
		</Header>
		<Menu
			items="global_menus"
			selected="global_pathParams.path"
			setMobileMenuFn="runFlow_setMobileMenu"
		/>
		<Content>
			<Fragment contents="global_mainContent" />
		</Content>
		<Footer>
			<Typography>footer</Typography>
		</Footer>
		<!-- <Sidebar></Sidebar> -->
	</Layout>
</Page>
